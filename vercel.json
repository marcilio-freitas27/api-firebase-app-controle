{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/login",
        "dest": "login.js"
      },
      {
        "src": "/logout",
        "dest": "logout.js"
      },
      {
        "src": "/con",
        "dest": "index.js"
      },
      {
        "src": "/dados",
        "dest": "index.js"
      }
    ],
    "functions": {
      "functions/verify.js": {
        "memory": 3008,
        "maxDuration": 30
      }
    }
  }